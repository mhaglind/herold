{
  "name": "herold",
  "version": "0.1.0",
  "type": "module",
  "description": "AI-powered family tree web application with natural language processing",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "vite --config src/client/vite.config.ts",
    "build": "npm run build:shared && npm run build:server && npm run build:client",
    "build:shared": "tsc --project src/shared/tsconfig.json",
    "build:server": "tsc --project src/server/tsconfig.json",
    "build:client": "vite build --config src/client/vite.config.ts",
    "start": "node dist/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "migrate": "tsx src/scripts/migrate-legacy-data.ts"
  },
  "keywords": [
    "family-tree",
    "genealogy",
    "ai",
    "natural-language-processing",
    "claude-api",
    "svg-generation",
    "typescript",
    "react",
    "node.js"
  ],
  "author": "MÃ¥rten Haglind <marten@haglind.com>",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@tanstack/react-query": "^4.35.7",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.30.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.19.17",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cypress": "^13.4.0",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "tsx": "^3.14.0",
    "typescript": "^5.9.2",
    "vite": "^4.5.14"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mhaglind/herold.git"
  },
  "bugs": {
    "url": "https://github.com/mhaglind/herold/issues"
  },
  "homepage": "https://github.com/mhaglind/herold#readme"
}
